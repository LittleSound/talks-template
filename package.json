{
  "name": "talks",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.5.0",
  "scripts": {
    "dev": "pnpm run picker dev --open",
    "build": "rimraf dist && pnpm run picker build",
    "build:all": "rimraf dist && pnpm -r run build",
    "export": "pnpm run picker export",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "picker": "PICKER_EDITOR=code esno scripts/picker.ts",
    "prepare": "git submodule update --init --recursive"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.22.0",
    "@eslint/config-inspector": "^0.5.1",
    "@iconify/json": "^2.2.226",
    "@slidev/cli": "^0.49.16",
    "@slidev/client": "^0.49.16",
    "@slidev/theme-default": "^0.25.0",
    "@types/node": "^20.14.10",
    "@types/prompts": "^2.4.9",
    "@types/seedrandom": "^3.0.5",
    "eslint": "^9.17.0",
    "eslint-plugin-format": "^0.1.3",
    "esno": "^4.8.0",
    "execa": "^9.5.2",
    "fast-glob": "^3.3.2",
    "markdown-it-magic-link": "^0.1.4",
    "playwright-chromium": "^1.45.1",
    "pnpm": "^9.5.0",
    "prettier-plugin-slidev": "^1.0.5",
    "prompts": "^2.4.2",
    "rimraf": "^6.0.1",
    "seedrandom": "^3.0.5",
    "taze": "^0.14.2",
    "typescript": "^5.5.3",
    "unocss": "^0.61.3",
    "vue-router": "^4.4.0",
    "vue-tsc": "^2.0.26"
  }
